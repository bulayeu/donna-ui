{
  "name": "donna-ui",
  "version": "0.0.1",
  "description": "Basic UI",
  "main": "dist/donna-ui.js",
  "scripts": {
    "dist-lib": "rollup -c rollup.config.js",
    "dist-docs": "build-storybook -o docs -s docs",
    "watch-lib": "rollup -c rollup.config.js -w",
    "watch-docs": "start-storybook -s ./stories/static -p 9999",
    "dev": "concurrently 'npm:watch-*'",
    "build": "npm run dist-lib && npm run dist-docs && npm run copy-static-docs",
    "copy-static-docs": "node scripts/copy-static-docs.js"
  },
  "author": "Pavel Bulayeu",
  "license": "MIT",
  "dependencies": {
    "autosize": "^4.0.2",
    "classnames": "^2.2.6",
    "clone": "^2.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-info": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "concurrently": "^4.0.1",
    "css-loader": "^0.28.9",
    "jest": "^22.4.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.9.3",
    "postcss-scss": "^1.0.3",
    "rollup": "^0.56.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-url": "^1.4.0",
    "rollup-watch": "^4.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2"
  }
}
